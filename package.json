{
  "name": "json-schema-openapi-typescript-code-generation",
  "version": "1.0.0",
  "repository": "git@github.com:Hajime-Suzuki/json-schema-openapi-typescript-code-generation.git",
  "author": "Hajime-Suzuki <hajime.doublebass@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn nodemon --watch build/src build/src/main.js",
    "check-updates": "ncu",
    "ts": "yarn json2ts -i 'schemas/orders/*.json' --cwd ./schemas/orders -o src/schemas/",
    "openApi": "node json-schema-to-openapi.js",
    "generate": "yarn openApi && yarn ts && yarn swagger-cli bundle -t yaml -o api.yml ./api-docs/api.yml",
    "downloadCommon": "aws s3 cp s3://docs.ochamo.space/schemas/common ./schemas/common --recursive",
    "validateOpenApi": "yarn swagger-cli validate --no-schema ./api.yml",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@openapi-contrib/json-schema-to-openapi-schema": "^1.2.0",
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.4",
    "@types/jest": "^26.0.16",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "esbuild": "^0.8.19",
    "esbuild-jest": "^0.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "fast-check": "^2.8.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.0",
    "json-schema-to-typescript": "^10.0.0",
    "nodemon": "^2.0.6",
    "npm-check-updates": "^10.2.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "jsonschema": "^1.4.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "shortid": "^2.2.16"
  }
}
